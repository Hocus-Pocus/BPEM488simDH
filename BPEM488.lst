004B97 7F4B97 CC 00 00                    ldd  #$0000      ; Load double accumulator with zero (0 volt ADC)            (CONVERT_ATD0)
004B9A 7F4B9A 3B                          pshd             ; Push to stack (V1)                                        (CONVERT_ATD0)
004B9B 7F4B9B FC 10 8E                    ldd  itrmAdc     ; Load double accumulator with "itrmAdc"                    (CONVERT_ATD0)
004B9E 7F4B9E CD 00 0A                    ldy  #$000A      ; Load index register Y with decimal 10                     (CONVERT_ATD0)
004BA1 7F4BA1 13                          emul             ; Multiply (D)x(Y)=>Y:D  (multiply "itrmAdc" by 10)         (CONVERT_ATD0)
004BA2 7F4BA2 3B                          pshd             ; Push to stack (V)                                         (CONVERT_ATD0)
004BA3 7F4BA3 CC 27 F6                    ldd  #$27F6      ; Load double accumulator with decimal 1023x10 (5 volt ADC) (CONVERT_ATD0)
004BA6 7F4BA6 3B                          pshd             ; Push to stack (V2)                                        (CONVERT_ATD0)
004BA7 7F4BA7 CC 00 00                    ldd  #$0000      ; Load double accumulator with zero (Low range degrees)     (CONVERT_ATD0)
004BAA 7F4BAA 3B                          pshd             ; Push to stack (Z1)                                        (CONVERT_ATD0)
004BAB 7F4BAB CC 01 90                    ldd  #$0190      ; Load double accumulator with decimal 40x10 (High range degrees) (CONVERT_ATD0)
