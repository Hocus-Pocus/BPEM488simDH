                                      ;*****************************************************************************************
005C5F 7F5C5F 1E 10 24 08 0B              brset ICflgs,Ch6alt,VSS2 ; If "Ch6alt" bit of "ICflgs" is set, branch to "VSS2:"
005C64 7F5C64 DC 5C                       ldd  ECT_TC6H            ; Load accu D with value in "ECT_TC6H"
005C66 7F5C66 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005C69 7F5C69 1C 10 24 08                 bset ICflgs,Ch6alt       ; Set "Ch6alt" bit of "ICflgs"
005C6D 7F5C6D 20 10                       bra  ECT6_ISR_Done       ; Branch to "ECT6_ISR_Done:"
005C6F 7F5C6F                         VSS2:
005C6F 7F5C6F DC 5C                       ldd  ECT_TC6H       ; Load accu D with value in "ECT_TC6H"
005C71 7F5C71 B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC6H"
005C74 7F5C74 7C 10 9E                    std  VSSprd         ; Copy result to "VSSprd"
005C77 7F5C77 1D 10 24 08                 bclr ICflgs,Ch6alt  ; Clear "Ch6alt" bit of "ICflgs"
005C7B 7F5C7B 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005C7F 7F5C7F                         ECT6_ISR_Done:
005C7F 7F5C7F 0B                          rti                ; Return from Interrupt
