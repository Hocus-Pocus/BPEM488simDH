0059C8 7F59C8 72 10 0C                    inc   rxmode        ; Increment "rxmode"(continue to next mode)
0059CB 7F59CB 0B                          rti                 ; Return from subroutine
0059CC 7F59CC                         RcvDataLSB:             ; "rxmode" = 8
0059CC 7F59CC D6 CF                       ldab  SCI0DRL       ; Load Accu B with value in "SCI0DRL"(data byte)
0059CE 7F59CE 7B 10 11                    stab  dataLSB       ; Copy to "dataLSB"
0059D1 7F59D1 B6 10 0E                    ldaa  pageID        ; Load accu A with value in "pageID"
0059D4 7F59D4 81 01                       cmpa  #$01          ; Compare with decimal 1 (send page 1)
0059D6 7F59D6 27 0A                       beq   StorePg1      ; If the Z bit of CCR is set, branch to StorePg1:
0059D8 7F59D8 81 02                       cmpa  #$02          ; Compare with decimal 2 (send page 2)
0059DA 7F59DA 27 4B                       beq   StorePg2      ; If the Z bit of CCR is set, branch to StorePg2:
0059DC 7F59DC 81 03                       cmpa  #$03          ; Compare with decimal 3 (send page 3)
