                                                              ; "Stallcnt" (stall counter)(offset = 998)
005D72 7F5D72 7C 10 22                    std  Stallcnt       ; Copy to "Stallcnt" (no crank or stall condition counter)
                                                              ; (1mS increments)
005D75 7F5D75 1E 10 24 04 12              brset ICflgs,Ch7_2nd,CAS_2nd ; If "Ch7_2nd" bit of "ICflgs" is set, branch to "CAS_2nd:"
005D7A 7F5D7A 1E 10 24 10 24              brset ICflgs,Ch7_3d,CAS_3d   ; If "Ch7_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
005D7F 7F5D7F DC 5E                       ldd   ECT_TC7H               ; Load accu D with value in "ECT_TC7H"
005D81 7F5D81 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005D84 7F5D84 1C 10 24 04                 bset  ICflgs,Ch7_2nd         ; Set "Ch7_2nd" bit of "ICflgs"
005D88 7F5D88 18 20 00 C0                 bra   CASDone                ; Branch to CASDone:
005D8C 7F5D8C                         CAS_2nd:
005D8C 7F5D8C DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005D8E 7F5D8E 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005D91 7F5D91 B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005D94 7F5D94 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005D97 7F5D97 1D 10 24 04                 bclr  ICflgs,Ch7_2nd  ; Clear "Ch7_2nd" bit of "ICflgs"
005D9B 7F5D9B 1C 10 24 10                 bset  ICflgs,Ch7_3d   ; Set "Ch7_3d" bit of "ICflgs"
005D9F 7F5D9F 18 20 00 A9                 bra   CASDone         ; Branch to CASDone:
005DA3 7F5DA3                         CAS_3d:
005DA3 7F5DA3 DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005DA5 7F5DA5 B3 10 17                    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC7H"
005DA8 7F5DA8 7C 10 1B                    std   CASprd2tk       ; Copy result to "CASprd2tk"
005DAB 7F5DAB F3 10 19                    addd  CASprd1tk       ; (A:B)+(M:M+1)_->A:B "CASprd2tk" + "CASprd1tk" = "CASprdtk"
005DAE 7F5DAE 1D 10 24 10                 bclr  ICflgs,Ch7_3d   ; Clear "Ch7_3d" bit of "ICflgs"
                                      ;*****************************************************************************************
                                      ; - All calculations that use the Crank Angle Sensor period need to know what the
                                      ;   resolution is. The timers are initalized with a 5.12uS resoluion but switched to
                                      ;   2.56uS resolution when the engine tranistions from crank mode to run mode.
                                      ;*****************************************************************************************
