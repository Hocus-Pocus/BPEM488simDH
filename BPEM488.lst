                                                            ; constant on buffer RAM page 1 (veBins_E)                 (FUEL_BURN_CALCS)
0055B6 7F55B6 EE EA 03 F0                 ldx   $03F0,Y     ; Load Accu X with value in buffer RAM page 1 (offset 1008)($03F0) (FUEL_BURN_CALCS)
                                                            ; ("InjPrFlo_F")                                           (FUEL_BURN_CALCS)
0055BA 7F55BA B7 56                       tfr  X,Y          ; "InjPrFlo_F"-> Accu Y                                    (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Calculate current fuel burn in Litres per Hour: (Injector open time over 1 second (FUEL_BURN_CALCS)
                                      ;   x 60 = Injector open time for 1 minute. Injector open time for 1 minute x injector (FUEL_BURN_CALCS)
                                      ;   flow rate per minute = injector flow for 1 minute. Injector flow for 1 minute x 60 (FUEL_BURN_CALCS)
                                      ;   = injector flow per hour. For integer math:                                  (FUEL_BURN_CALCS)
                                      ;    ((("FDsec"/10)*6)*InjPrFlo_F)/10,000= "LpH"(Litres per hour x 10)           (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
0055BC 7F55BC FC 10 CC                    ldd  FDsec       ; "FDsec"->Accu D (Fuel delivery pulse width total for 1 Sec (mS*10) (FUEL_BURN_CALCS)
0055BF 7F55BF CE 00 0A                    ldx  #$000A      ; Decimal 10 -> Accu X                                      (FUEL_BURN_CALCS)
0055C2 7F55C2 18 10                       idiv             ;(D)/(X)->Xrem->D "FDsec"/10                                (FUEL_BURN_CALCS)
0055C4 7F55C4 B7 54                       tfr  X,D         ; Result-> Accu D                                           (FUEL_BURN_CALCS)
0055C6 7F55C6 13                          emul             ; (D)*(Y)->Y:D ("FDsec"/10)*"InjPrFlo_F"                    (FUEL_BURN_CALCS)
0055C7 7F55C7 CE 27 10                    ldx  #$2710      ; Decimal 10,000-> Accu X                                   (FUEL_BURN_CALCS)
0055CA 7F55CA 11                              ediv             ;(Y:D)/)X)->Y;Rem->D (("FDsec"/10)*"InjPrFlo_F")/10,000="LpH" (FUEL_BURN_CALCS)
0055CB 7F55CB 7D 10 CA                    sty  LpH         ; Copy result to "LpH" (Litres per hour x 10)               (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Calculate current fuel burn in Kilometers per Litre                          (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
0055CE 7F55CE FC 10 A2                    ldd  KPH         ; "KPH" -> Accu D                                           (FUEL_BURN_CALCS)
0055D1 7F55D1 CD 00 0A                    ldy  #$000A      ; Decimal 10 -> Accy Y                                      (FUEL_BURN_CALCS)
0055D4 7F55D4 13                          emul             ; (D)*(Y)->Y:D "KPH"*10                                     (FUEL_BURN_CALCS)
0055D5 7F55D5 FE 10 CA                    ldx  LpH         ; "LpH"-> Accu X                                            (FUEL_BURN_CALCS)
0055D8 7F55D8 11                              ediv             ;(Y:D)/)X)->Y;Rem->D ("KPH"*10)/LpH = KmpL              (FUEL_BURN_CALCS)
0055D9 7F55D9 7D 10 D4                    sty  KmpL        ; Result -> "KmpL"                                          (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Convert fuel burn in Kilometers per Litre to Miles per Gallon Imperial       (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
0055DC 7F55DC FC 10 D4                    ldd  KmpL        ; "KmpL" -> Accu D                                          (FUEL_BURN_CALCS)
0055DF 7F55DF CD 00 64                    ldy  #$0064      ; Decimal 100 -> Accy Y                                     (FUEL_BURN_CALCS)
0055E2 7F55E2 13                          emul             ; (D)*(Y)->Y:D "KmpL"*100                                   (FUEL_BURN_CALCS)
0055E3 7F55E3 B7 46                       tfr  D,Y         ; Result Lo word-> Accu Y                                   (FUEL_BURN_CALCS)
0055E5 7F55E5 CC 01 1A                    ldd  #$011A      ; Decimal 282-> Accu D (2.82 is conversion factor)          (FUEL_BURN_CALCS)
0055E8 7F55E8 13                          emul             ; (D)*(Y)->Y:D ("KmpL"*100)*282                             (FUEL_BURN_CALCS)
0055E9 7F55E9 CE 27 00                    ldx  #$2700      ; Decimal 10,000 -> Accu X                                  (FUEL_BURN_CALCS)
0055EC 7F55EC 11                              ediv             ;(Y:D)/)X)->Y;Rem->D (("KmpL"*100)*282)/10,000= "MpG"   (FUEL_BURN_CALCS)
0055ED 7F55ED 7D 10 EA                    sty   MpG        ; Result -> "MpG"                                           (FUEL_BURN_CALCS)
