                                      ;***************************************************************************************** (CRV_INTERP)
006388 7F6388 3D                         rts   ; Return from subroutine                                                (CRV_INTERP)
                                      ;**************************************************************************************
006389 7F6389                         CmpValPos:
                                      ;*****************************************************************************************
                                      ; - CrvCmpVal is positive. Starting at the beginning of the row, loop through until the
                                      ;   first positive value is found.
                                      ;*****************************************************************************************
006389 7F6389                         PosFind:
006389 7F6389 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
00638C 7F638C 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006392 7F6392 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
